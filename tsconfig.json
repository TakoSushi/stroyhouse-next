{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "dist/types/**/*.ts", "src/app/components/SectionAboutCompany/SectionAboutCompany.tsx",
    // "src/app/components/SectionCallback/SectionCallback.jsx", "src/app/components/SectionQuote/SectionQuote.tsx", "src/app/components/SectionRules/SectionRules.tsx"
    ],
  "exclude": [
    "node_modules"
  ]
}
